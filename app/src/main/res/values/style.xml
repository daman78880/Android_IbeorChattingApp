<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="welcomeStyle">
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginEnd">30dp</item>
    </style>
    <style name="welcomeStyleBtn">
        <item name="android:layout_marginStart">25dp</item>
        <item name="android:layout_marginEnd">25dp</item>
        <item name="android:padding">20dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="troubleLayoutStyleOne">
        <item name="android:fontFamily">@font/gotham_rounded</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="troubleLayoutStyleTwo">
        <item name="android:fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="signinByEmailStyleOne">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="signInStyleOne">
        <item name="android:layout_marginTop">10dp</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="signInOtpStyleOne">
        <item name="android:layout_marginTop">10dp</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginStart">18sp</item>
        <item name="android:layout_marginEnd">18sp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
    </style>
    <style name="signRegisterMail">
        <item name="android:layout_marginTop">10dp</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginStart">18sp</item>
        <item name="android:layout_marginEnd">18sp</item>
    </style>
    <style name="choice_select">
        <item name="android:layout_marginTop">10dp</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginStart">18sp</item>
        <item name="android:layout_marginEnd">18sp</item>
    </style>

    <style name="dob_style">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="dob_style_two">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="stOne">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#f5f7ff</item>
    </style>
    <style name="stTwo">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="BottomSheet" parent="@style/Widget.Design.BottomSheet.Modal">

        <item name="android:typeface">monospace</item>
        <item name="android:background">@drawable/cornercircle</item>
    </style>

    <style name="BaseBottomSheetDialog" parent="@style/Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:typeface">monospace</item>
        <item name="bottomSheetStyle">@style/BottomSheet</item>
    </style>

    <style name="BottomSheetDialogTheme" parent="BaseBottomSheetDialog" />
    <style name="constraint_only">
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:layout_marginStart">18dp</item>
        <item name="android:layout_marginEnd">18dp</item>
    </style>
    <style name="forTitle">
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:layout_marginStart">18dp</item>
        <item name="android:layout_marginEnd">18dp</item>
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="userInforStyle">
        <item name="fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <array name="slider_values">
        <item>18</item>
        <item>50</item>
    </array>
    <array name="height_slider_values">
        <item>4.0</item>

    </array>

    <style name="newsdpStyle"
        >
        <item name="android:fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="newsdpStyleSimple"
        >
        <item name="android:fontFamily">@font/gotham_rounded</item>
        <item name="android:textSize">@dimen/_11sdp</item>
    </style>

    <style name="Myslider" parent="@style/Widget.MaterialComponents.Slider">
        <item name="labelStyle">@style/My_Tooltip</item>
        <item name="materialThemeOverlay">@style/ThemeOverlay.Slider</item>
    </style>

    <style name="My_Tooltip" parent="Widget.MaterialComponents.Tooltip">
        <item name="backgroundTint">@color/gulabiColor</item>
    </style>
    <style name="ThemeOverlay.Slider" parent="">
        <item name="colorOnPrimary">@color/white</item>
    </style>
</resources>