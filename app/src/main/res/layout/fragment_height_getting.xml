<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundNormal"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".modules.editProfile.GettingUserData.HeightGettingFragment">
    <include layout="@layout/toolbar_filter_setting"
        android:id="@+id/toolbarGettingHeightUserData"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <com.google.android.material.slider.Slider
        android:id="@+id/normalContinuousSlider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_14sdp"
        android:layout_marginEnd="@dimen/_14sdp"
        android:valueFrom="3.0"
        android:valueTo="7.11"
        app:labelBehavior="floating"
        app:trackColorActive="#cc49d5"
        app:trackColorInactive="#cccccc"
        app:thumbColor="#cc49d5"
        style="@style/Myslider"
        app:thumbStrokeColor="@color/white"
        android:value="4.0"
        android:layout_marginTop="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarGettingHeightUserData" />

</androidx.constraintlayout.widget.ConstraintLayout>