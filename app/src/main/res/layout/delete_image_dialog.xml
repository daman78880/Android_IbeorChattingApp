<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        >
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/delelteBtnDeleteImage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/delete"
        android:textColor="@color/red"
        android:elevation="10dp"
        android:padding="15sp"
        android:layout_marginTop="20dp"

        android:background="@drawable/et_phone_number_enter_round"
        android:textSize="18sp"
        android:gravity="center_horizontal|center_vertical"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/signInOtpStyleOne"
        />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/cancelBtnDeleteImage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CANCEL"
        android:textColor="@color/black"
        android:elevation="10dp"
        android:padding="15sp"
        android:background="@drawable/et_phone_number_enter_round"
        android:textSize="18sp"
        android:gravity="center_horizontal|center_vertical"
        style="@style/signInOtpStyleOne"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        app:layout_constraintTop_toBottomOf="@+id/delelteBtnDeleteImage"
        />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>