                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardUseCompatPadding="true"
                android:elevation="@dimen/_2sdp"
                android:background="@color/white"
                app:cardElevation="@dimen/_2sdp"
                app:cardCornerRadius="@dimen/_13sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_7ssp"
                    >
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/layoutLogoTopProfileSetting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/round_like_card"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/imgLogoProfileSetting"
            android:layout_width="@dimen/_110sdp"
            android:layout_height="@dimen/_70sdp"
            android:src="@drawable/logo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtLogoTempProfileSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GET PREMIUM PLUS"
                android:textColor="@color/black"
                android:layout_marginBottom="@dimen/_7sdp"
                android:fontFamily="@font/gotham_rounded"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgLogoProfileSetting"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/layoutSupperLikeTopProfileSettingg"
                        android:layout_width="@dimen/_120sdp"
                        android:layout_height="@dimen/_67sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/round_like_card"
                        app:layout_constraintTop_toBottomOf="@+id/layoutLogoTopProfileSetting"
                        app:layout_constraintStart_toStartOf="parent"
                        >

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgSuperProfileSettingg"
                            android:layout_width="@dimen/_35sdp"
                            android:layout_height="@dimen/_35sdp"
                            android:src="@drawable/super_like_img"
                            android:layout_marginTop="@dimen/_10sdp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/get_super_likes"
                            android:textColor="@color/skyBule"
                            android:gravity="center"
                            android:elevation="@dimen/_15sdp"
                            android:textSize="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_35sdp"
                            app:layout_constraintTop_toTopOf="@+id/imgSuperProfileSettingg"
                            app:layout_constraintStart_toStartOf="@+id/imgSuperProfileSettingg"
                            app:layout_constraintEnd_toEndOf="@+id/imgSuperProfileSettingg"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/layoutBoostTopProfileSetting"
                        android:layout_width="@dimen/_120sdp"
                        android:layout_height="@dimen/_67sdp"
                        android:background="@drawable/round_like_card"
                        app:layout_constraintTop_toTopOf="@+id/layoutSupperLikeTopProfileSettingg"
                        app:layout_constraintEnd_toEndOf="parent"
                        >
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgBoostProfileSettingg"
                            android:layout_width="@dimen/_40sdp"
                            android:layout_height="@dimen/_40sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:src="@drawable/boost_stiker"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/get_super_likes"
                            android:textColor="@color/skyBule"
                            android:gravity="center"
                            android:elevation="@dimen/_15sdp"
                            android:textSize="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_40sdp"
                            app:layout_constraintTop_toTopOf="@+id/imgBoostProfileSettingg"
                            app:layout_constraintStart_toStartOf="@+id/imgBoostProfileSettingg"
                            app:layout_constraintEnd_toEndOf="@+id/imgBoostProfileSettingg"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_10ssp"
                        android:background="@drawable/round_like_card"
                        android:layout_marginTop="@dimen/_10sdp"
                        app:layout_constraintTop_toBottomOf="@+id/layoutSupperLikeTopProfileSettingg"
                        >
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgInviteTempProfileSettingg"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:src="@drawable/share_app"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/txtInviteTextTempProfilteSettingg"
                            />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/txtInviteTextTempProfilteSettingg"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/invite"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            app:layout_constraintTop_toTopOf="@+id/imgInviteTempProfileSettingg"
                            app:layout_constraintBottom_toBottomOf="@+id/imgInviteTempProfileSettingg"
                            app:layout_constraintStart_toEndOf="@+id/imgInviteTempProfileSettingg"
                            app:layout_constraintEnd_toStartOf="@+id/imgHornTempProfileSettingg"
                            />
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgHornTempProfileSettingg"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:src="@drawable/share_celebration_horn"
                            app:layout_constraintStart_toEndOf="@+id/txtInviteTextTempProfilteSettingg"
                            app:layout_constraintTop_toTopOf="@+id/imgInviteTempProfileSettingg"
                            app:layout_constraintBottom_toBottomOf="@+id/imgInviteTempProfileSettingg"
                            app:layout_constraintEnd_toEndOf="parent"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <androidx.appcompat.widget.AppCompatTextView
