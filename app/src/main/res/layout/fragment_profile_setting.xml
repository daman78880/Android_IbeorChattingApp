                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/account_settings"
                android:textSize="@dimen/_14sdp"
                style="@style/userInforStyle"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@+id/cardLogoProfileSetting"
                app:layout_constraintStart_toStartOf="@+id/cardLogoProfileSetting"
                />
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardAccountSettingProfileSetting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/white"
                app:cardUseCompatPadding="true"
                app:cardElevation="@dimen/_1sdp"
                app:cardCornerRadius="@dimen/_7sdp"
                app:layout_constraintTop_toBottomOf="@+id/txtAccountSettingTempProfileSetting"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                <LinearLayout
                    android:id="@+id/phoneNumberLayoutSetting"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/_10sdp"
                    >
                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/phone_number"
                    android:fontFamily="@font/gotham_rounded"
                    android:textSize="@dimen/_12ssp"
                    android:textColor="#4d4d4d"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdp"
                    />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txtMobileNumberSetting"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:layout_gravity="center"
                    android:fontFamily="@font/gotham_rounded"
                    android:textSize="@dimen/_12ssp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_weight="1"
                    />
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/right_erro"
                        android:layout_gravity="center"
                        />
                </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:id="@+id/loginLayoutSetting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/login_methods"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"

                            />
                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_erro"
                            />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:id="@+id/mailLayoutSetting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/email"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/txtEmailSetting"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="end"
                            />
                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_erro"
                            />
                    </LinearLayout>
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtCardSettingTemp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_10sdp"
                android:text="@string/a_verified_phone_number_and_email_help_secure_your_account"
                android:layout_marginTop="@dimen/_10ssp"
                app:layout_constraintTop_toBottomOf="@+id/cardAccountSettingProfileSetting"
                app:layout_constraintStart_toStartOf="parent"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtCardNotificationTemp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/notifications"
                android:textSize="@dimen/_14sdp"
                style="@style/userInforStyle"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10ssp"
                app:layout_constraintTop_toBottomOf="@+id/txtCardSettingTemp"
                app:layout_constraintStart_toStartOf="parent"
                />
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardNotificationSettingProfileSetting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/white"
                app:cardUseCompatPadding="true"
                app:cardElevation="@dimen/_1sdp"
                app:cardCornerRadius="@dimen/_7sdp"
                app:layout_constraintTop_toBottomOf="@+id/txtCardNotificationTemp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:id="@+id/mailNotificationLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/email"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_weight="1"
                            />
                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_erro"
                            android:layout_gravity="center"
                            />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:id="@+id/puchNotificationLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/push_notification"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"

                            />
                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/right_erro"
                            />
                    </LinearLayout>
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txtCardOtherTemp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/notifications"
                android:textSize="@dimen/_14sdp"
                style="@style/userInforStyle"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10ssp"
                app:layout_constraintTop_toBottomOf="@+id/cardNotificationSettingProfileSetting"
                app:layout_constraintStart_toStartOf="parent"
                />
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardOtherSettingProfileSetting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/white"
                app:cardUseCompatPadding="true"
                app:cardElevation="@dimen/_1sdp"
                app:cardCornerRadius="@dimen/_7sdp"
                app:layout_constraintTop_toBottomOf="@+id/txtCardOtherTemp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/ads"
                            android:textStyle="bold"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="@color/black"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_weight="1"
                            />
                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/switchForAds"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_21sdp"
                            android:thumb="@drawable/thumb"
                            app:track="@drawable/track"
                            android:checked="true"
                            app:trackTint="#00b294"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"/>

                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Block User"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"

                            />

                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/faq"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/contact_us"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#e6e6e6"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/_10sdp"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/terms_of_use"
                            android:fontFamily="@font/gotham_rounded"
                            android:textSize="@dimen/_12ssp"
                            android:textColor="#4d4d4d"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            />
                    </LinearLayout>
                    <View
